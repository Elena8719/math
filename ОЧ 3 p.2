    "from math import factorial\n",
    "def combinations(k,n): \n",
    "    return int(factorial(n)/(factorial(k)*factorial(n-k)))\n",
    "p1=((combinations(2,3)*combinations(0,5))/combinations(2,8))*((combinations(3,5)*combinations(1,7))/combinations(4,12))\n"
